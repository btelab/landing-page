    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/svg": "^0.3.0",
    "@nuxtjs/tailwindcss": "^4.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "dotenv": "^16.0.3",
    "postcss": "^8.3.5",
